<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lee.calendar" >

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.lee.calendar.Calendar"
            android:label="Calendar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.example.lee.calendar.ListViewActivity"
            android:label="ListViewActivity">
        </activity>
        <activity android:name="com.example.lee.calendar.AddEventActivity"
            android:label="AddEventActivity"></activity>
    </application>

</manifest>
